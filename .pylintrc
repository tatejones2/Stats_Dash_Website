[MASTER]
load-plugins=

[MESSAGES CONTROL]
disable=missing-module-docstring,
        missing-function-docstring,
        line-too-long,
        too-few-public-methods,
        import-error

[FORMAT]
max-line-length=100

[BASIC]
good-names=i,j,k,ex,Run,_,df,st

[DESIGN]
max-args=7
max-locals=15
max-returns=6
max-branches=12
max-statements=50